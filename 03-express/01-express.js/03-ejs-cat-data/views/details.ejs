  <title>Details</title>
</head>
<body>
  <html>
<body>
    <p><%= cats[0].pic %></p>
      <h2>I'm : <%= cats[0].name %></h2>
    <% for (var x in cats) { %>
        <h2>I'm : <%= cats[x].name %></h2>
        <h3>Favorite food: <%= cats[x].food %></h3>
        <h3>Age: <%= cats[x].age %></h3>
        <h3>Sleeping Spots: <%= cats[x].sleep1 %></h3>
        <hr>
    <% } %>
</body>
</html>
</body>
</html>